generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Request {
  id        String   @id
  method    String
  route     String
  input     String   @db.Text
  output    String   @db.Text
  status    Int
  createdAt DateTime
  updatedAt DateTime

  @@map("requests")
}

model PointOfInterest {
  id        String   @id
  name      String
  xCoord    Int
  yCoord    Int
  createdAt DateTime

  @@map("points_of_interests")
}
